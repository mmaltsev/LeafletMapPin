{
  "name": "marker-maker",
  "version": "0.1.0",
  "description": "Small service for outlining Leaflet pin markers via HTML5 Canvas",
  "main": "src/main.js",
  "scripts": {
    "test": "./node_modules/.bin/jasmine",
    "docs": "./node_modules/.bin/jsdoc --readme docs/README.md main.js -d docs",
    "uglify": "./node_modules/.bin/uglifyjs --output dist/marker-maker.min.js --compress --mangle -- src/main.js",
    "build": "npm run clean:dist && npm run test && npm run docs && npm run uglify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmaltsev/MarkerMaker.git"
  },
  "keywords": [
    "Leaflet",
    "marker",
    "pin",
    "map"
  ],
  "author": {
    "name": "Maxim Maltsev",
    "email": "maltsevmn@ya.ru"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmaltsev/MarkerMaker/issues"
  },
  "homepage": "https://github.com/mmaltsev/MarkerMaker#readme",
  "devDependencies": {
    "chai": "^4.1.1",
    "jasmine": "^2.8.0",
    "jsdoc": "^3.5.4",
    "jsdom": "^11.2.0",
    "mocha": "^3.5.0",
    "uglify-es": "^3.0.28"
  }
}
